{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "arcanedev/log-viewer": "4.7.*",
        "barryvdh/laravel-cors": "^0.11.3",
        "caffeinated/modules": "5.1.*",
        "doctrine/dbal": "^2.9",
        "dougsisk/laravel-country-state": "^2.1",
        "fideloper/proxy": "^4.0",
        "knovators/authentication": "1.*",
        "knovators/httplogger": "1.0.*",
        "knovators/masters": "1.*",
        "knovators/media": "1.*",
        "knovators/support": "1.*",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.2",
        "laravel/tinker": "^1.0",
        "prettus/l5-repository": "^2.6",
        "yajra/laravel-datatables-oracle": "9.0.*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "secure-http": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@git.knovator.in:knovators/logger.git"
        },
        {
            "type": "vcs",
            "url": "git@git.knovator.in:knovators/authentication.git"
        },
        {
            "type": "vcs",
            "url": "git@git.knovator.in:knovators/media.git"
        },
        {
            "type": "vcs",
            "url": "git@git.knovator.in:knovators/support.git"
        },
        {
            "type": "vcs",
            "url": "git@git.knovator.in:knovators/master.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Libraries/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
